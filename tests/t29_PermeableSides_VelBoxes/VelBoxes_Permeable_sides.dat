		rho 		= 	10 			# density
		eta 		= 	1e18 			# viscosity
		alpha  	 	= 	3e-5 
		

		# Thermal properties
        k           =   100        		# conductivity
        Cp          =   1e6        		# heat capacity - should be artificially large for sticky air
	<MaterialEnd>
	
	# Define properties of oceanic crust
	<MaterialStart>
